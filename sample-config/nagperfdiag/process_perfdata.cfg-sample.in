#
# Config File for process_perfdata.pl
#
# $Id: process_perfdata.cfg-sample.in 423 2008-04-15 17:35:59Z pitchfork $
#
# process_perfdata.pl Timout 
#
TIMEOUT = 5
#
# Use RRDs Perl Module
#
USE_RRDs = 1 
#
# 
#
RRDPATH = @PERFDATA_DIR@
#
#
#
RRDTOOL = @RRDTOOL@
#
#
#
CFG_DIR = @CFG_DIR@
#
#
#
RRD_HEARTBEAT = 8460 
#
#
#
RRA_CFG = @CFG_DIR@rra.cfg
#
#
#
RRA_STEP = 60
#
#
#
LOG_FILE = @PERFDATA_LOG@
#
# Loglevel 0=silent 1=normal 2=debug
#
LOG_LEVEL = @DEBUG@
#
# XML encoding
# The supported encodings are ISO-8859-1, UTF-8 and US-ASCII.
# http://www.php.net/xml-parser-create
XML_ENC = UTF-8
#
# NORMALIZE_VALUES
# Converting to base unit before storing values in RRD.
# For example kB in Bytes and ms in s
#
NORMALIZE_VALUES = 1
#
# HANDLE_MULTILINE = 0
# Enable the processing of multiline plugin performance data
#
HANDLE_MULTILINE = 0
